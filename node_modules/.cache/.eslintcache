yle: 'currency', currency: 'USD' });\n    }\n\n    const removeItem = () => {\n\n    }\n\n    const editItem = () => {\n        //\n    }\n\n    return (\n        <div>\n            <p>\n                {cartItems.length === 1 ? `You have ${cartItems.length} item in your cart` : \n                (cartItems.length > 1 ? `You have ${cartItems.length} items in your cart`: \n                <EmptyCart/>)}\n            </p>\n\n            <Wrapper>\n                <div className='product-list'>\n\n                \n                {cartItems.map((item, index) => (\n                    <div key={index} style={{}}>\n                        {item.product.length === 1 ? \n                        <div style={{display: 'flex'}}>\n                            <img src={item.product.image} alt={item.product.name} style={{width: '150px'}}/>\n                            <div>\n                                <h3>{item.product.name}</h3>\n                                <p>${item.product.price}</p>\n                                <h5>Qty: {item.qty}</h5> \n                            </div>\n                        </div> \n                        : \n                        <div style={{display: 'flex', justifyContent: 'space-between', margin: '15px 40px', borderTop: '1px solid #404040'}}>\n                            \n                            <img src={item.product.image} alt={item.product.name} style={{width: '150px'}}/>\n                            <div>\n                                <h3>{item.product.name}</h3>\n                                <h5>Qty: {item.qty}</h5> \n                            </div>\n                            <p>${item.product.price}</p>\n                        </div>\n                        \n                        }\n\n                    </div>\n                ))}\n                </div>\n                <div className='product-checkout'>\n                    <h4>Summary</h4>\n                    <div style={{display: 'flex', justifyContent: 'space-between'}}>\n                        <p>Subtotal</p>\n                        <p>{formatMoney(totalPrice)}</p>\n                    </div>\n                    <div style={{display: 'flex', justifyContent: 'space-between'}}>\n                        <p>Estimated Shipping & Handling</p>\n                        <p>$0.00</p>\n                    </div>\n                    <div style={{display: 'flex', justifyContent: 'space-between'}}>\n                        <p>Tax</p>\n                        <p>{formatMoney(tax)}</p>\n                    </div>\n                    \n                    <p style={{backgroundColor: '#131921', color: 'white', padding: '10px 0'}}>Total: {formatMoney(totalPrice + tax)}</p>\n                    <div>\n                        <button>Checkout</button>\n                    </div>\n                </div>\n            </Wrapper>\n            <Footer/>\n        </div>\n    )\n}\n\nexport default Cart;\n\n\nconst Wrapper = styled.div`\npadding-bottom: 130px;\n\n.product-checkout {\n    margin: 0 20px;\n}\n\n@media only screen and ${breakpoint.device.desktop} {\n\n   \n    display: flex;\n    \n    \n    .product-list {\n        width: 60%;\n    }\n    \n    .product-checkout {\n        width: 40%;\n        margin-right: 40px;\n    }\n}\n`","/Users/ruthmathieu/Desktop/Current-Project/laced-up/src/client/components/Shop.js",[],"/Users/ruthmathieu/Desktop/Current-Project/laced-up/src/client/components/Home.js",["76"],"import React from 'react';\nimport { useParams } from \"react-router-dom\";\nimport { Link } from 'react-router-dom';\nimport styled from 'styled-components';\nimport breakpoint from '../../breakpoint';\nimport NewItem from './NewItem';\n\n//comp\nimport Footer from './Footer';\n\n//images\nimport heroLeft from '../../images/desktop-bg-hero-two.png';\nimport heroRight from '../../images/desktop-bg-hero.png';\nimport one from '../../images/products/new-release-collection-2.png';\nimport two from '../../images/products/