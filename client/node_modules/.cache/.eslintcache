         item.id\n        ))\n    }\n\n    const editItem = () => {\n        //\n    }\n\n    return (\n        <div>\n\n            { cartItems.length >= 1 ? \n            <div>\n                <p>\n                {cartItems.length === 1 ? `You have ${cartItems.length} item in your cart` : \n                cartItems.length > 1 ? `You have ${cartItems.length} items in your cart` : '' }\n            </p>\n\n            <Wrapper>\n                <div className='product-list'>\n\n                \n                {cartItems.map((item, index) => (\n                    <div key={index}>\n                        {item.product.length === 1 ? \n                        <div>\n                        <div style={{display: 'flex'}}>\n                            <img src={item.product.image} alt={item.product.name} style={{width: '150px'}}/>\n                            <div>\n                                <h3>{item.product.name}</h3>\n                                <p>${item.product.price}</p>\n                                <h5>Qty: {item.qty}</h5> \n                            </div>\n                        </div> \n                    </div>\n                        : \n                        <div style={{display: 'flex', justifyContent: 'space-between', margin: '15px 40px', borderTop: '1px solid #404040'}}>\n                            \n                            <img src={item.product.image} alt={item.product.name} style={{width: '150px'}}/>\n                            <div>\n                                <h3>{item.product.name}</h3>\n                                <h5>Qty: {item.qty}</h5> \n                                <p onClick={removeItem}>Remove</p>\n                            </div>\n                            <p>${item.product.price}</p>\n                            \n                        </div>\n                        \n                        }\n                        \n\n                    </div>\n                ))}\n                \n                \n                </div>\n                <div className='product-checkout'>\n                        <h4>Summary</h4>\n                        <div style={{display: 'flex', justifyContent: 'space-between'}}>\n                            <p>Subtotal</p>\n                            <p>{formatMoney(totalPrice)}</p>\n                        </div>\n                        <div style={{display: 'flex', justifyContent: 'space-between'}}>\n                            <p>Estimated Shipping & Handling</p>\n                            <p>$0.00</p>\n                        </div>\n                        <div style={{display: 'flex', justifyContent: 'space-between'}}>\n                            <p>Estimated Tax</p>\n                            <p>{formatMoney(tax)}</p>\n                        </div>\n                        \n                        <p style={{backgroundColor: '#131921', color: 'white', padding: '10px 0'}}>Total: {formatMoney(totalPrice + tax)}</p>\n                        <div>\n                            <button>Checkout</button>\n                        </div>\n                    </div>\n            </Wrapper> \n            </div>\n                :\n                <EmptyCart/>\n            }\n            <Footer/>\n        </div>\n    )\n}\n\nexport default Cart;\n\n\nconst Wrapper = styled.div`\npadding-bottom: 130px;\n\n.product-checkout {\n    margin: 0 20px;\n}\n\n@media only screen and ${breakpoint.device.desktop} {\n\n   \n    display: flex;\n    \n    \n    .product-list {\n        width: 60%;\n    }\n    \n    .product-checkout {\n        width: 40%;\n        margin-right: 40px;\n    }\n}\n`","/Users/alanna.matieu/Desktop/laced-up/client/src/client/components/Shop.js",[],"/Users/alanna.matieu/Desktop/laced-up/client/src/client/components/Navigation.js",[],"/Users/alanna.matieu/Desktop/laced-up/client/src/client/components/products/WomenShoes.js",["94"],"import React from 'react';\nimport styled from 'styled-components';\nim